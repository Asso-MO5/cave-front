{
  "name": "cave-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev  -p 8080",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
